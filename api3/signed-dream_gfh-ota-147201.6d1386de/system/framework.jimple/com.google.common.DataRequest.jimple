public interface  com.google.common.DataRequest extends java.lang.Object
{

    public abstract int getRequestType();

    public abstract boolean isForeground();

    public abstract boolean isSubmission();

    public abstract boolean readResponseData(java.io.DataInput) throws java.io.IOException;

    public abstract boolean requestImmediately();

    public abstract void writeRequestData(java.io.DataOutput) throws java.io.IOException;
}
